<%= form_tag action: "preview" do %>
    <%= text_field :add_feed, :url %>
    <%= submit_tag "追加" %>
<% end %>

<% if @articles.present? %>
    <% @articles.each do |article| %>
        <%= link_to article.title, article.url %> <br />
        <%= article.published %> <br />
        <%== article.content.lines[0] %> <br />
        <br />

    <% end %>
<% end %>
